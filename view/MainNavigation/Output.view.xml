<mvc:View controllerName="DemoUXapp.controller.MainNavigation.Table"
    height="100%"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:ui="sap.ui.table"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    displayBlock="true">

    <ui:Table id="idProductsTable"
        rows="{path:'/results', templateShareable: true}"
        alternateRowColors="true"
        visibleRowCount="15"
        selectionBehavior="RowSelector"
        selectionMode="None"
        columnHeaderVisible="false"
        enableSelectAll="false"
        width="16rem">

        <ui:extension>
           
        </ui:extension>

        <ui:columns>
            <ui:Column>
                <m:Label text="Technical Name" />
                <ui:template>
                    <m:Text text="{TechnicalName}" />
                </ui:template>
            </ui:Column>
        </ui:columns>
        <ui:rowActionTemplate>

        </ui:rowActionTemplate>
        <ui:footer>

        </ui:footer>
    </ui:Table>

</mvc:View>